{
  "name": "root",
  "private": true,
  "workspaces": [
    "packages/*",
    "palett/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "prewatch": "npm run bootstrap",
    "upint": "yarn upgrade-interactive --latest",
    "build": "lerna exec --parallel -- rollup -c ../../rollup.config.js",
    "build-specific": "lerna exec --scope=crostab -- rollup -c ../../rollup.config.js",
    "watch": "lerna exec --parallel -- rollup -c ../../rollup.config.js -w",
    "watch-specific": "lerna exec --scope=kalorie -- rollup -c ../../rollup.config.js -w"
  },
  "devDependencies": {
    "@analys/crostab": "^0.3.6",
    "@babel/core": "^7.9.0",
    "@babel/plugin-external-helpers": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.8.3",
    "@babel/plugin-proposal-private-methods": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@spare/logger": "^0.8.2",
    "@valjoux/strategies": "^0.1.10",
    "lerna": "^3.20.2",
    "mocha": "^8.0.1",
    "rollup": "^2.6.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-fileinfo": "^0.1.2",
    "rollup-plugin-terser": "^6.1.0"
  }
}
